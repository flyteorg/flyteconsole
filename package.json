{
    "name": "flyteconsole",
    "version": "0.45.0",
    "description": "The web UI for the Flyte platform",
    "repository": {
        "type": "git",
        "url": "http://github.com/lyft/flyteconsole.git"
    },
    "main": "index.js",
    "scripts": {
        "clean": "rm -rf dist",
        "prebuild": "yarn run clean",
        "build": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' webpack --config webpack.config.ts --mode=development",
        "build:prod": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' NODE_ENV=production webpack --config webpack.config.ts --mode=production",
        "build:storybook": "build-storybook",
        "start": "node -r dotenv/config index.js",
        "storybook": "start-storybook -p 6006",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --ignore-path .eslintignore --write \"**/*.+(js|jsx|ts|tsx|json)\"",
        "fix": "yarn lint --fix && yarn format",
        "mockApi": "node ./mockApi",
        "tdd": "yarn run test --watch --verbose false",
        "test": "NODE_ENV=test jest --config=jest.config.js",
        "test-coverage": "NODE_ENV=test jest --coverage --config=jest.config.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "keywords": [
        "react",
        "lyft"
    ],
    "author": "Lyft <flyte-eng@lyft.com>",
    "license": "Apache-2.0",
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ]
    },
