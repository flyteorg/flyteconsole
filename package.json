{
    "name": "@flyteconsole/oss",
    "version": "1.3.5",
    "private": true,
    "description": "The web UI for the Flyte platform",
    "repository": {
        "type": "git",
        "url": "https://github.com/flyteorg/flyteconsole"
    },
    "workspaces": [
        "packages/*",
        "website"
    ],
    "scripts": {
        "postinstall": "husky install",
        "packages": "make pack",
        "clean": "yarn workspace @flyteconsole/client-app clean",
        "start": "yarn workspace @flyteconsole/client-app start",
        "start:prod": "yarn workspace @flyteconsole/client-app start:prod",
        "build:prod": "yarn workspace @flyteconsole/client-app build:prod",
        "build:storybook": "build-storybook",
        "generate:package": "yarn workspace @flyteconsole/generator start",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "storybook": "start-storybook -p 6006",
        "test": "NODE_ENV=test jest",
        "test:clear": "jest --clearCache",
        "test-coverage": "NODE_ENV=test jest --coverage"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged --debug",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "keywords": [
        "react",
        "lyft"
    ],
    "author": "Lyft <flyte-eng@lyft.com>",
    "license": "Apache-2.0",
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json}": [
            "eslint --fix",
            "prettier --ignore-path .eslintignore --write"
        ]
    },
    "dependencies": {
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "babel-loader": "^8.2.5",
        "babel-polyfill": "^6.26.0",
        "chalk": "^2.0.1",
        "cheerio": "1.0.0-rc.2",
        "compression-webpack-plugin": "^9.2.0",
        "cookie-parser": "^1.4.3",
        "copy-webpack-plugin": "^11.0.0",
        "express": "^4.14.0",
        "express-static-gzip": "^0.3.2",
        "fork-ts-checker-webpack-plugin": "^7.2.1",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.2",
        "morgan": "^1.8.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-hot-loader": "^4.1.2",
        "react-router": "^5.3.4",
        "react-router-dom": "^5.3.4",
        "ts-loader": "^9.2.6",
        "ts-node": "^8.0.2",
        "tslib": "^2.4.1",
        "typescript": "^4.9.4",
        "use-react-router": "^1.0.7",
        "wait-on": "^6.0.1",
        "webpack": "^5.68.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.1",
        "webpack-merge": "^5.8.0",
        "webpack-node-externals": "^3.0.0",
        "webpack-stats-plugin": "^1.0.3"
    },
    "devDependencies": {
        "@babel/core": "~7.16.12",
        "@babel/preset-env": "~7.16.11",
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@flyteorg/flyteidl": "^1.3.0",
        "@material-ui/core": "^4.2.0",
        "@material-ui/icons": "^4.2.1",
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/commit-analyzer": "^8.0.1",
        "@semantic-release/git": "^9.0.0",
        "@semantic-release/github": "^7.0.5",
        "@semantic-release/npm": "^7.0.5",
        "@semantic-release/release-notes-generator": "^9.0.1",
        "@storybook/addon-actions": "^6.4.19",
        "@storybook/addon-essentials": "^6.4.19",
        "@storybook/addon-interactions": "^6.4.19",
        "@storybook/addon-links": "^6.4.19",
        "@storybook/builder-webpack5": "^6.4.19",
        "@storybook/manager-webpack5": "^6.4.19",
        "@storybook/react": "^6.4.19",
        "@storybook/testing-library": "^0.0.9",
        "@testing-library/jest-dom": "^5.5.0",
        "@testing-library/react": "^10.0.3",
        "@testing-library/react-hooks": "^7.0.2",
        "@typescript-eslint/eslint-plugin": "^5.15.0",
        "@typescript-eslint/parser": "^5.15.0",
        "eslint": "^8.11.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.3.0",
        "jest": "^26.0.0",
        "lint-staged": "^13.1.0",
        "msw": "^0.24.1",
        "prettier": "^2.5.1",
        "serve-static": "^1.12.3",
        "source-map-loader": "^3.0.1",
        "ts-jest": "^26.3.0"
    },
    "resolutions": {
        "@babel/cli": "~7.16.0",
        "@babel/core": "~7.16.12",
        "@babel/plugin-proposal-class-properties": "~7.16.7",
        "@babel/plugin-proposal-decorators": "~7.16.7",
        "@babel/plugin-proposal-object-rest-spread": "~7.16.7",
        "@babel/preset-env": "~7.16.11",
        "@babel/preset-react": "~7.16.7",
        "@babel/preset-typescript": "~7.16.7",
        "minimist": "1.2.6"
    },
    "packageManager": "yarn@3.2.1"
}
