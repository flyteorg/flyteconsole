    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transformer-svg": "^2.0.1",
    "lint-staged": "^13.0.3",
    "long": "^5.2.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "msw": "^2.2.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "nodemon": "^2.0.19",
    "parse5": "^7.1.2",
    "postcss": "^8.4.25",
    "postcss-loader": "^7.3.3",
    "prettier": "^2.8.1",
    "prom-client": "^14.0.1",
    "protobufjs": "^6.11.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "semantic-release": "^21.0.7",
    "source-map-loader": "^4.0.1",
    "string-template": "^1.0.0",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsc-alias": "^1.8.7",
    "tsc-watch": "^6.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.5.5",
    "wait-on": "^7.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-dev-server": "^4.15.1",
    "webpack-hot-middleware": "^2.25.4",
    "webpack-merge": "^5.10.0",
