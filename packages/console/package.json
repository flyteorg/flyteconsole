    "@types/react-router-dom": "^5.3.3",
    "@types/react-virtualized": "^9.21.4",
    "@types/serve-static": "^1.7.31",
    "@types/shallowequal": "^0.2.3"
  },
  "resolutions": {
    "react": "^16.13.1",
