    "main": "./dist/index.js",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "license": "Apache-2.0",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "files": [
        "LICENSE",
        ".yalc",
        "README.md",
        "dist",
        "lib",
        "src",
        "node_modules",
        "tsconfig.build.tsbuildinfo"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/flyteorg/flyteconsole.git",
        "directory": "packages/console"
    },
    "scripts": {
        "debug": "NM_DEBUG_LEVEL=2 yarn",
        "bundledeps": "npx bundle-dependencies update",
        "clean": "rm -rf dist && rm -rf lib && rm -rf node_modules && rm -rf tsconfig.build.tsbuildinfo",
        "clean:all": "rm -rf node_modules && yarn clean",
        "build:watch": "run -T tsc-watch --noClear -p ./tsconfig.es.build.json --onSuccess \"yarn build\"",
        "build": "yarn build:esm && yarn build:cjs && yarn build:cp",
        "build:esm": "run -T tsc --module esnext --outDir ./lib --project ./tsconfig.es.build.json && tsc-alias  -p ./tsconfig.es.build.json",
        "build:cjs": "run -T tsc --project ./tsconfig.build.json && tsc-alias -p ./tsconfig.build.json",
        "build:cp": "run -T wait-on ./lib && cp -R ./src/assets ./lib && run -T wait-on ./dist && cp -R ./src/assets ./dist",
        "test": "NODE_ENV=test jest"
    },
    "installConfig": {
        "hoistingLimits": "workspaces"
    },
