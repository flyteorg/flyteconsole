    "chartjs-plugin-datalabels": "2.0.0",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.0.8",
    "cronstrue": "^1.31.0",
    "d3-dag": "^0.3.4",
    "d3-shape": "^1.2.2",
    "dagre": "0.8.5",
    "dagre-d3": "^0.6.4",
    "debug": "2.6.9",
    "dom-helpers": "5.2.1",
    "fuzzysort": "^1.1.1",
    "intersection-observer": "^0.7.0",
    "js-yaml": "^3.13.1",
    "linkify-it": "^2.2.0",
