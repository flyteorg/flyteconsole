{
    "name": "@flyteoss/flyteidl",
    "version": "0.0.3",
    "description": "Flyteconsole basics flyteidl setup",
    "main": "./dist/index.js",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "license": "Apache-2.0",
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/flyteorg/flyteconsole.git",
        "directory": "packages/flyteidl"
    },
    "files": [
        "LICENSE",
        "README.md",
        "dist",
        "lib",
        "node_modules",
        "tsconfig.build.tsbuildinfo"
    ],
    "installConfig": {
        "hoistingLimits": "workspaces"
    },
    "scripts": {
        "build": "yarn build:esm && yarn build:cjs",
        "build:esm": "run -T tsc --module esnext --outDir lib --project ./tsconfig.build.json",
        "build:cjs": "run -T tsc --project ./tsconfig.build.json",
        "test": "NODE_ENV=test jest"
    },
    "dependencies": {
        "@flyteorg/flyteidl": "^1.3.0",
        "protobufjs": "~6.11.3"
    }
}
